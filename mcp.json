{
  "mcpProxy": {
    "baseURL": "http://localhost:8001",
    "addr": ":8001",
    "name": "MCP Proxy",
    "version": "1.0.0",
    "type": "streamable-http",
    "options": {
      "panicIfInvalid": false,
      "logEnabled": true
    }
  },
  "mcpServers": {
    "skogai-tools": {
      "command": "/home/skogix/skogai/tools/mcp/server/index.js",
      "args": ["/home/skogix/skogai/tools"],
      "env": {},
      "transportType": "stdio"
    },
    "skogai-librarian": {
      "command": "/home/skogix/skogai/tools/mcp/server/index.js",
      "args": ["/home/skogix/skogai/tools", "librarian"],
      "env": {},
      "transportType": "stdio"
    },
    "skogai-memory": {
      "url": "https://memory.skogai.se/mcp",
      "transportType": "streamable-http"
    }
  }
}
